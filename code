EXIT_EXC EQU 64

WRITE_EXC EQU 66

STACK_ADRS EQU 0X1000

LOAD_ADRS EQU 0XF000

NIL EQU 0

SP EQU R15

WR EQU R14

BP EQU R13

ORG LOAD_ADRS

start main_

main_ LDW SP, #STACK_ADRS

LDQ NIL, BP

STW BP,-(SP)

LDW BP, SP

LDW R0, #5

STW R0, (SP)+

LDW R0, #5

LDW R1, -(SP)

MPC WR 

ADQ 10,WR

CMP R1, R0

JEQ #run-$-2

LDW R0, #0

TRP R0

LDW SP, BP

LDW BP, (SP)+

LDW WR, #EXIT_EXC

TRP WR

LDW WR, #main_

JEA (WR)

run LDW R0,#1

JEA (WR)

