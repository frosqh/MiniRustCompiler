EXIT_EXC EQU 64

WRITE_EXC EQU 66

STACK_ADRS EQU 0X1000

LOAD_ADRS EQU 0XF000

NIL EQU 0

SP EQU R15

WR EQU R14

BP EQU R13

ORG LOAD_ADRS

start main_

main_ LDW SP, #STACK_ADRS

LDQ NIL, BP

STW BP,-(SP)

LDW BP, SP

LDW R0, #86

STW R0, (SP)+

LDW R0, #34

LDW R1, -(SP)

ADD R1, R0, R0

TRP R0

LDW SP, BP

LDW BP, (SP)+

LDW WR, #EXIT_EXC

TRP WR

LDW WR, #main_

JEA (WR)

