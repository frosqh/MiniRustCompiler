EXIT_EXC EQU 64

WRITE_EXC EQU 66

STACK_ADRS EQU 0X1000

LOAD_ADRS EQU 0XF000

NIL EQU 0SP EQU R15

WR EQU R14

BP EQU R13

ORD LOAD_ADRS

start main

main_ LDW SP, #STACK_ADRS

LDQ NIL, BP

STW BP,-(SP)

LDW BP, SP

LDW WR, #WRITE_EXC

TRP WR

LDW SP, BP

LDW BP, (SP)+

LDW WR, #EXIT_EXC

TRP WR

LDW WR, #debut

JEA (WR)

